{
    "reference":{
        "Character->Element-Keyword Equivalence":{
            "g":"generic including empty",
            "#":"empty",
            "e":"generic excluding empty",
            "w":"water",
            "l":"lava",
            "o":"obsidian"
        },
        "Symbols":{
            "LHS Operators":{
                "/":{
                    "Description":"When two g are right next to eachother",
                    "Usage":"Unspecified",
                    "order":"Does Not Matter",
                    "parameters":["g","g"]
                },
                "-":{
                    "Description":"When two g are one tile apart",
                    "Usage":"Unspecified",
                    "Order":"Does Not Matter",
                    "Parameters":["g","g"]
                },
                "_":{
                    "Description":"When e is above g",
                    "Usage":["Gravity","Unspecified"],
                    "Order":"Matters",
                    "Parameters":{"1":"g","2":"e"}
                }
            },
            "equivalence":"=",
            "RHS Operators":{
                " ":"spacing for code to distinquish arguments",
                "><":"elements are swapped",
                "|":"Seperates results when needed",
                ">":"What both elements turn into",
                ">>":"What specific operand from left hand side turns into"
            }
        } 
    },
    "rules":[
        "w _ # = ><",
        "l _ # = ><",
        "w + l = w >> # | l >> o"
    ]
}